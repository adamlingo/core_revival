<p id="notice"><%= notice %></p>

<h1>Listing Payroll Records</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @payroll_records.each do |payroll_record| %>
      <tr>
        <td><%= link_to 'Show', payroll_record %></td>
        <td><%= link_to 'Edit', edit_payroll_record_path(payroll_record) %></td>
        <td><%= link_to 'Destroy', payroll_record, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Payroll record', new_payroll_record_path %>
