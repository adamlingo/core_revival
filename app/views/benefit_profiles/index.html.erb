<p id="notice"><%= notice %></p>

<h1>Listing Benefit Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @benefit_profiles.each do |benefit_profile| %>
      <tr>
        <td><%= benefit_profile.name %></td>
        <td><%= benefit_profile.company_id %></td>
        <td><%= link_to 'Show', benefit_profile %></td>
        <td><%= link_to 'Edit', edit_benefit_profile_path(benefit_profile) %></td>
        <td><%= link_to 'Destroy', benefit_profile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Benefit profile', new_benefit_profile_path %>
