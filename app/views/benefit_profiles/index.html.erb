<p id="notice"><%= notice %></p>

<h1>Listing Benefit Profiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Company</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @benefit_profiles.each do |benefit_profile| %>
      <tr>
        <td><%= benefit_profile.provider %></td>
        <td><%= benefit_profile.company.name %></td>
        
      </tr>
    <% end %>
  </tbody>
</table>

<br>