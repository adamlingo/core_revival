<p id="notice"><%= notice %></p>

<h1>Listing Employee Benefits</h1>

<table>
  <thead>
    <tr>
      <th>Employee Name</th>
      <th>PTO Balance</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
        <% @employee_benefits.each do |employee_benefit| %>
          <tr>
              <td><%= employee_benefit.employee.last_name %></td>
              <td><%= employee_benefit.employee.first_name%></td>
              <td><%= employee_benefit.pto_balance%></td>


          </tr>
      <% end %>
  </tbody>
</table>

<br>